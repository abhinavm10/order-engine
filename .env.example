# Server
PORT=3000
NODE_ENV=development

# Database
DATABASE_URL=postgres://your_user:your_password@localhost:5432/order_engine

# Redis
REDIS_URL=redis://localhost:6379

# Queue Settings
QUEUE_CONCURRENCY=10
MAX_RETRIES=3

# Logging
LOG_LEVEL=info

# Mock DEX (optional seed for deterministic tests)
MOCK_SEED=

# Rate Limiting
RATE_LIMIT=30

# WebSocket
PING_INTERVAL=20000
PONG_TIMEOUT=10000
